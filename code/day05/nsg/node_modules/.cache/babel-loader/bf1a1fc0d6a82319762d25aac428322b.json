/label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"goodsname\"\r\n          placeholder=\"确认密码\"\r\n          aria-label=\"确认密码\"\r\n          v-model=\"checkpsw\"\r\n          @blur=\"checkpassword\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"goodspirce\">手机号码:</label>\r\n        <input type=\"tel\" id=\"goodspirce\" placeholder=\"tel\" aria-label=\"tel\" v-model=\"tel\" />\r\n        <label for=\"goodsspirce\">性别:</label>\r\n        <select class=\"custom-select\" id=\"inputGroupSelect01\" v-model=\"gender\">\r\n          <option value=\"male\">男</option>\r\n          <option value=\"female\">女</option>\r\n        </select>\r\n      </div>\r\n      <div class>\r\n        <label for=\"goodsname\">生日:</label>\r\n        <input type=\"date\" id=\"goodsname\" placeholder=\"生日\" aria-label=\"生日\" v-model=\"birthday\" />\r\n      </div>\r\n      <div class>\r\n        <label for=\"goodsname\">邮箱地址:</label>\r\n        <input type=\"email\" id=\"goodsname\" placeholder=\"邮箱地址\" aria-label=\"邮箱地址\" v-model=\"mail\" />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"exampleFormControlTextarea1\">备注:</label>\r\n        <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" v-model=\"remark\"></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\" @click=\"submit\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      name: \"\",\r\n      callname: \"\",\r\n      psw: \"\",\r\n      checkpsw: \"\",\r\n      tel: \"\",\r\n      gender: \"\",\r\n      birthday: \"\",\r\n      mail: \"\",\r\n      remark: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    password() {\r\n      if (this.psw.length < 6 || this.psw.length > 12) {\r\n        alert(\"密码长度不符合标准\");\r\n        this.psw = \"\";\r\n      }\r\n    },\r\n    checkpassword() {\r\n      if (this.psw != this.checkpsw && this.psw != \"\") {\r\n        alert(\"两次输入的密码不一致\");\r\n        this.checkpsw = \"\";\r\n      }\r\n    },\r\n    async checkname() {\r\n      if (this.name.trim()) {\r\n        let result = await this.$axios({\r\n          method: \"get\",\r\n          url: \"http://localhost:5454/users/check\",\r\n          params: {\r\n            name: this.name\r\n          }\r\n        });\r\n        if (!result.data.code) {\r\n          alert(\"用户名可用\");\r\n        } else {\r\n          alert(\"用户名已被使用\");\r\n          this.name = \"\";\r\n        }\r\n      }\r\n    },\r\n    async submit() {\r\n      let opt = {\r\n        name: this.name,\r\n        callname: this.callname,\r\n        psw: this.psw,\r\n        checkpsw: this.checkpsw,\r\n        tel: this.tel,\r\n        gender: this.gender,\r\n        birthday: this.birthday,\r\n        mail: this.mail,\r\n        remark: this.remark\r\n      };\r\n      let isok = true;\r\n      for (let item in opt) {\r\n        if (opt[item] == \"\") {\r\n          isok = false;\r\n        }\r\n      }\r\n      if (isok) {\r\n        await this.$axios({\r\n          method: \"post\",\r\n          url: \"http://localhost:5454/users/reg\",\r\n          params: opt\r\n        });\r\n        this.$router.push({ path: \"UserList\" });\r\n      } else {\r\n        alert(\"请填入所有信息\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n#adduser {\r\n  padding: 20px;\r\n}\r\n\r\n.tips {\r\n  color: #999;\r\n}\r\n\r\n#adduser {\r\n  label {\r\n    width: 100px !important;\r\n    text-align: right;\r\n    margin-right: 20px;\r\n  }\r\n\r\n  select {\r\n    margin-left: 50px;\r\n    width: 100px;\r\n  }\r\n\r\n  input {\r\n    width: 300px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}