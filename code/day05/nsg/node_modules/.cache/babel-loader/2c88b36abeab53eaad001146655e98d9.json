nder\">\r\n          <option value=\"male\">男</option>\r\n          <option value=\"female\">女</option>\r\n        </select>\r\n      </div>\r\n      <div class>\r\n        <label for=\"goodsname\">生日:</label>\r\n        <input type=\"date\" id=\"goodsname\" placeholder=\"生日\" aria-label=\"生日\" v-model=\"birthday\" />\r\n      </div>\r\n      <div class>\r\n        <label for=\"goodsname\">邮箱地址:</label>\r\n        <input type=\"email\" id=\"goodsname\" placeholder=\"邮箱地址\" aria-label=\"邮箱地址\" v-model=\"mail\" />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"exampleFormControlTextarea1\">备注:</label>\r\n        <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" v-model=\"remark\"></textarea>\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary\" @click=\"changeinfo\">Submit</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data: function() {\r\n    return {\r\n      name: \"\",\r\n      callname: \"\",\r\n      psw: \"\",\r\n      tel: \"\",\r\n      gender: \"\",\r\n      birthday: \"\",\r\n      mail: \"\",\r\n      remark: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    async changeinfo() {\r\n      let result = await this.$axios({\r\n        method: \"get\",\r\n        url: \"http://localhost:5454/users/check\",\r\n        params: {\r\n          query: {\r\n            name: this.name\r\n          },\r\n          data: {\r\n            callname: this.callname,\r\n            psw: this.psw,\r\n            tel: this.tel,\r\n            gender: this.gender,\r\n            birthday: this.birthday,\r\n            mail: this.mail,\r\n            remark: this.remark\r\n          }\r\n        }\r\n      });\r\n      window.console.log(result);\r\n    }\r\n  },\r\n  async mounted() {\r\n    let { name } = this.$route.query;\r\n    window.console.log(name);\r\n    let result = await this.$axios({\r\n      method: \"get\",\r\n      url: \"http://localhost:5454/users/check\",\r\n      params: {\r\n        name: name\r\n      }\r\n    });\r\n    let info = result.data.data[0];\r\n    this.name = info.name;\r\n    this.callname = info.callname;\r\n    this.psw = info.psw;\r\n    this.tel = info.tel;\r\n    this.gender = info.gender;\r\n    this.birthday = info.birthday;\r\n    this.mail = info.mail;\r\n    this.remark = info.remark;\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"stylus\" scoped>\r\n#changeinfo {\r\n  padding: 20px;\r\n}\r\n\r\n.tips {\r\n  color: #999;\r\n}\r\n\r\n#changeinfo {\r\n  label {\r\n    width: 100px !important;\r\n    text-align: right;\r\n    margin-right: 20px;\r\n  }\r\n\r\n  select {\r\n    margin-left: 50px;\r\n    width: 100px;\r\n  }\r\n\r\n  input {\r\n    width: 300px;\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}